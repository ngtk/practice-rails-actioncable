<div class="ui text container">
  <h1 class="ui dividing header">Authenticate as one of the following users</h1>

  <p>
    <ul class="ui list">
      <% @users.each do |user| %>
        <li><%= link_to user.name, session_url(user_id: user.id), method: :post %></li>
      <% end %>
    </ul>
  </p>
</div>
